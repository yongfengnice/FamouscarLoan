<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="swipelistviewstyle">
        <attr name="right_width" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="CircleProgressBar">
        <attr name="ringNormalWidth" format="dimension" />
        <attr name="ringWidth" format="dimension" />
        <attr name="ringNormalColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressTextVisibility" format="boolean" />
        <attr name="maxProgress" format="integer" />
        <attr name="circleStyle">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SwitchButton">
        <attr name="bmWidth" format="dimension"></attr>
        <attr name="bmHeight" format="dimension"></attr>
    </declare-styleable>

</resources>