package com.famous.zhifu.loan.holder;

import com.famous.zhifu.loan.view.dialog.SelectFilterPopup;

import android.widget.CompoundButton;
import android.widget.ImageView;
import android.widget.TextView;

public class InvestViewHolder {
	private CompoundButton cbButton;
	private String key;
	private TextView label;
	private ImageView gouView;
	private int position;
	private String name;
	private SelectFilterPopup pop;
	
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public TextView getLabel() {
		return label;
	}
	public void setLabel(TextView label) {
		this.label = label;
	}
	public ImageView getGouView() {
		return gouView;
	}
	public void setGouView(ImageView gouView) {
		this.gouView = gouView;
	}
	public int getPosition() {
		return position;
	}
	public void setPosition(int position) {
		this.position = position;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public SelectFilterPopup getPop() {
		return pop;
	}
	public void setPop(SelectFilterPopup pop) {
		this.pop = pop;
	}
	public CompoundButton getCbButton() {
		return cbButton;
	}
	public void setCbButton(CompoundButton cbButton) {
		this.cbButton = cbButton;
	}
}
